<body style="overflow-x: hidden;">
   <div class="container m-5">
    <div class="row">
        <div class="col-md-12 d-flex justify-content-center">
            <h1>Coupons & Links</h1>
        </div>
        <div class="row">
            <div class="col-md-12">
            <!-- table starts here -->
            <div class="table-responsive">

            <table class="table mt-3">
                <thead>
                  <tr>
                    
                    <th scope="col">Choose Category</th>
                    <th scope="col">Coupon value</th>
                    <th scope="col">Coupon Value</th>
                    <th scope="col">Apply</th>
                    
                  </tr>
                </thead>
                <tbody>
                    <form action="#" method="post">
                  <tr>
                   
                    <td>

                       
            <select class="form-select" name="roomcategory"  id="roomcategory">
              <option selected disabled value="">Choose a Category</option>
              {{range .category}}<option value="{{.Category_Name}}">{{.Category_Name}}</option>{{end}}
            </select>

                    </td>
                    <td>
                        <input type="text" name="couponcode" id="couponcode" placeholder="Example: OFF25%">
                    </td>
                    <td>
                        <input type="text" name="couponvalue" id="couponvalue" placeholder="Example:25">
                        
                    </td>
                    <td>
                        <button type="submit" class="btn btn-sm btn-danger">AddCoupon</button>
                    </td>
                 
                  </tr>
                </form>
                </tbody>
              </table>
            <!-- tabel ends here -->
            </div>
        </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            
        </div>
    </div>


   </div>
